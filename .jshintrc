{
	curly: true,
	eqeqeq: true,
	eqnull: true,
	bitwise: true,
	immed: true,
	latedef: true,
	noarg: true,
	nonbsp: true,
	quotmark: 'single',
	undef: true,
	unused: false,
	maxparams: 5,
	browser: true,
	globals: {
		ct: true,
		Polymer: true,
	},
	extract: 'auto',
	ignores: ['web/lib/**', 'web/app/**', 'web/e2e-tests/**']
}